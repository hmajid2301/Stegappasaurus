{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "globals": {
    "fetch": false
  },
  "rules": {
    "arrow-body-style": "warn",
    "class-methods-use-this": "off",
    "import/newline-after-import": ["error", { "count": 2 }],
    "no-console": 0
  }
}
