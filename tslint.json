{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "__tests__/**/*"
    ]
  },
  "jsRules": {},
  "rules": {
    "jsx-no-lambda": false,
    "no-var-requires": false
  },
  "rulesDirectory": []
}