{
  "name": "stegappasaurus",
  "version": "0.1.0",
  "scripts": {
    "android": "react-native run-android",
    "android-build-apk": "cd android && ./gradlew assembleRelease --no-daemon",
    "android-build-aab": "cd android && ./gradlew bundleRelease --no-daemon",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file ./index.js --bundle-output ./android/app/src/main/assets/index.bundle --sourcemap-output ./android/app/src/main/assets/index.map --assets-dest ./android/app/src/main/res/",
    "android-clean": "cd android && ./gradlew clean",
    "android-debug": "cd android && ./gradlew assembleDebug",
    "android-lint": "cd android && ./gradlew -Pci --console=plain :app:lintDebug -PbuildDir=lint",
    "clean-tests": "yarn tests -u",
    "code-formatter": "prettier src/**/*.{ts,tx,tsx}",
    "code-formatter-check": "prettier --check src/**/*.{ts,tx,tsx}",
    "compiler-check": "tsc --project . --noEmit --pretty --skipLibCheck",
    "coverage": "jest --coverage",
    "log": "react-native log-android",
    "lint": "tslint src/**/*.{ts,tx,tsx} -c tslint.json",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "tests": "jest",
    "watch-tests": " jest --watch"
  },
